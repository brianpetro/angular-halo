<div ng-controller="ProjectCtrl">
	<h1>Listing projects</h1>

	<table>
		<thead>
		  <tr>
		    <th>Project Name</th>
		    <th>Project Author</th>
		  </tr>
		</thead>

		<tbody>
		  <tr ng-repeat="project in projects">
		  	<td>{{ project.details.name }}</td>
		  	<td>{{ project.details.author }}</td>
		  </tr>
		</tbody>
	</table>

	<br />

	<%= link_to 'New Project', new_project_path %>
</div>
