<div ng-controller="EntriesCtrl">
	<h1>Listing entries</h1>

	<table>
		<thead>
		  <tr>
		    <th>Entry_Data</th>
		    <th>Entry_Data_Title</th>
		    <th>Entry_Data_Summary</th>
		  </tr>
		</thead>

		<tbody>
		  <tr ng-repeat="entry in entries">
		    <td>{{ entry.data }}</td>
		    <td>{{ entry.data.title }}</td>
		    <td>{{ entry.data.summary }}</td>
		  </tr>
		</tbody>
	</table>

	<br />

	<%= link_to 'New Entry', new_entry_path %>
</div>
